{
    "globals": {
        "angular": false,
        "$": false,
        "sjcl": false
    },
    "rules": {
        "comma-dangle": [2, "always"],
        "no-cond-assign": 1,
        "no-console": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-empty": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": 1,
        "no-extra-semi": 2,
        "no-func-assign": 1,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-sparse-arrays": 1,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 1,
        "valid-typeof": 2,
        "accessor-pairs": 1,
        "array-callback-return": 1,
        "block-scoped-var": 1,
        "complexity": [2, 8],
        "consistent-return": 1,
        "curly": [1, "all"],
        "default-case": 1,
        "dot-location": [1, "property"],
        "dot-notation": 1,
        "eqeqeq": 1,
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-case-declarations": 1,
        "no-div-regex": 1,
        "no-else-return": 1,
        "no-empty-function": 1,
        "no-empty-pattern": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-label": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 1,
        "no-implied-eval": 1,
        "no-iterator": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-loop-func": 1,
        "no-magic-numbers": [1, {
            "ignore": [0, 1, -1]
        }],
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-octal": 1,
        "no-octal-escape": 1,
        "no-param-reassign": [1, {
            "props": false
        }],
        "no-proto": 1,
        "no-redeclare": 1,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-assign": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unmodified-loop-condition": 1,
        "no-unused-expressions": 1,
        "no-unused-labels": 1,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-useless-escape": 1,
        "no-void": 1,
        "no-warning-comments": 1,
        "no-with": 1,
        "radix": 1,
        "vars-on-top": 0,
        "wrap-iife": 1,
        "yoda": 1,
        "init-declarations": [1, "always"],
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undefined": 1,
        "no-unused-vars": 1,
        "no-use-before-define": 0,
        "comma-style": [2, "last"],
        "computed-property-spacing": [1, "never"],
        "brace-style": 2,
        "camelcase": 1,
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": 1,
        "eol-last": 1,
        "func-names": 0,
        "func-style": 0,
        "id-blacklist": [2, "llenar"],
        "id-length": 0,
        "id-match": [0, "^[a-z]+([A-Z][a-z]+)*$"],
        "indent": [0, 2],
        "jsx-quotes": 0,
        "key-spacing": 1,
        "keyword-spacing": 1,
        "linebreak-style": 0,
        "lines-around-comment": 1,
        "max-depth": 1,
        "max-len": [1, 130],
        "max-nested-callbacks": 1,
        "max-params": 0,
        "max-statements": 0,
        "max-statements-per-line": 1,
        "new-cap": 1,
        "new-parens": 1,
        "newline-after-var": 0,
        "newline-before-return": 1,
        "newline-per-chained-call": 1,
        "no-array-constructor": 1,
        "no-bitwise": 1,
        "no-continue": 1,
        "no-inline-comments": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multiple-empty-lines": 1,
        "no-negated-condition": 1,
        "no-nested-ternary": 1,
        "no-new-object": 1,
        "no-plusplus": 1,
        "no-restricted-syntax": 1,
        "no-spaced-func": 1,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 1,
        "object-curly-spacing": 1,
        "object-property-newline": 0,
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "operator-assignment": 1,
        "operator-linebreak": [1, "before"],
        "padded-blocks": [1, "never"],
        "quote-props": 0,
        "quotes": [2, "single"],
        "semi": 0,
        "semi-spacing": 0,
        "sort-vars": 0,
        "space-before-blocks": 1,
        "space-before-function-paren": 1,
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "wrap-regex": 1
    }
}
